<div class="ED_new_form_box">
<%= form_with(model: ouvrage, local: true) do |form| %>
  <% if ouvrage.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(ouvrage.errors.count, "error") %> prohibited this ouvrage from being saved:</h2>

      <ul>
      <% ouvrage.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="ED_new_form_field">
    <%= form.label :name, "Nom de l'ouvrage" %>
    <%= form.text_field :name %>
  </div>
  <div class="ED_new_form_field">
    <%= form.label :maitre_oeuvre, "MO de l'ouvrage" %>
    <%= form.text_field :maitre_oeuvre %>
  </div>
  <div class="ED_new_form_field">
    <%= form.label :budget, "budget de l'ouvrage" %>
    <%= form.text_field :budget %>
  </div>
  <div class="ED_new_form_field">
    <%= form.label :adresse, "Adresse de l'ouvrage" %>
    <%= form.text_field :adresse, id: "user_input_autocomplete_address" %>
  </div>
  <div class="ED_new_form_field">
    <%= form.label :photo, "Photo de l'ouvrage" %>
    <%= form.file_field :photo %>
  </div>

  <div class="ED_new_form_field_submit">
    <%= form.submit 'Valider '%>
  </div>
<% end %>
</div>

