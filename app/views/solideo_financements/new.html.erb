              <label><%= file_field_tag :file, accept: '.csv'%> <i class="fas fa-plus-circle"></i><span class="SF-new-input-text"> Ajouter T1 exec 2018</span> </label>
              <%= hidden_field_tag :date_year, 2018 %>
              <%= hidden_field_tag :date_month, 3 %>
            </div>
            <div class="SF_new_form_field_submit">
              <%= button_tag(type: "submit") do %> <i class="fas fa-check"></i> <% end %>
            </div>
          <%end%> 
        <% else %>
          <div class="SF-new-file-exist">
            <span>T1 2018 exec <i class="fas fa-check"></i> </span>
