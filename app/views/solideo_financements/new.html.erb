<div class="SD_page">
  <div class="SD_home_button">
    <%= link_to solideo_financements_path do %><i class="fas fa-chevron-left"></i><i class="fas fa-home"></i>Retour<%end %>
  </div>
 
  <h1 class="SD_title">Le financement de la solideo</h1>
    <div class="row">
    <div class="col-md-6">
       <div class="SF-new-box">
         <% if SolideoFinancement.where('date = ? AND montant_prevu > ?', Date.new(2018,3), 0).nil? || SolideoFinancement.where('date = ? AND montant_prevu > ?', Date.new(2018,3), 0).empty? %> 
           <%=form_tag import_solideo_financements_path, multipart: true do %>
            <div class="text-center SF-new-input">
              <label><%= file_field_tag :file, accept: '.csv' %> <i class="fas fa-plus-circle"></i><span class="SF-new-input-text"> Ajouter T1 prévu 2018 </span></label>
              <%= hidden_field_tag :date_year, 2018 %>
              <%= hidden_field_tag :date_month, 3 %>
            </div>
            <div class="SF_new_form_field_submit">
              <%= button_tag(type: "submit") do %> <i class="fas fa-check"></i> <% end %>
            </div>
           <%end%> 
         <% else %>
          <div class="SF-new-file-exist">
           <span> T1 2018 prévu <i class="fas fa-check"></i></span>
          </div>
         <div class="SF-new-file-delete">
           <%= form_tag deleteall_solideo_financements_path do %>
           <%= hidden_field_tag :date,Date.new(2018,3) %>
           <%= button_tag(type: "submit") do %> <i class="fas fa-trash-alt"></i><%end%>
           <% end %>
         </div>
         <% end %>
       </div>
    </div>
    <div class="col-md-6">
        <div class="SF-new-box">
         <% if SolideoFinancement.where('date = ? AND montant > ?', Date.new(2018,3), 0).nil? || SolideoFinancement.where('date = ? AND montant > ?', Date.new(2018,3), 0).empty? %> 
