<p id="notice"><%= notice %></p>

<h1>Ouvrages Financements</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Ouvrage</th>
      <th>Name</th>
      <th>Montant</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ouvrages_financements.each do |ouvrages_financement| %>
      <tr>
        <td><%= ouvrages_financement.date %></td>
        <td><%= ouvrages_financement.ouvrage.name %></td>
        <td><%= ouvrages_financement.name %></td>
        <td><%= ouvrages_financement.montant %></td>
        <td><%= link_to 'Show', ouvrages_financement %></td>
        <td><%= link_to 'Edit', edit_ouvrages_financement_path(ouvrages_financement) %></td>
        <td><%= link_to 'Destroy', ouvrages_financement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

